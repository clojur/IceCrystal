<?xml version="1.0" ?>
<sequence>
    <setProgram>
        <module name="globalsShaderFS"/>
        <module name="atmosphereShaderFS"/>
        <module name="oceanBrdf"/>
        <module name="treeBrdf"/>
        <module name="clouds"/>
        <module name="this.material"/>
    </setProgram>
    <drawTerrain name="this.terrain" mesh="this.grid" culling="true"/>
</sequence>
